{
    "version":2,
    "builds":[
        {
            "src":"/blog/wsgi.py",
            "use":"@vercel/python",
            "config":{"maxLambdaSize":"50mb","runtime":"3.10"}
        },
        {
            "src":"build_files.sh",
            "use":"@vercel/static-build",
            "config":{"distDir":"staticfiles_build"}
        }

            ],

    "routes":[
        {
            "src":"/static/(.*)",
            "dest":"/static_build/$1"
        },
        {
            "src":"/(.*)",
            "dest":"/blog/wsgi.py"
        }
    ]
    
}